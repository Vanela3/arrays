#include<stdio.h>
int main () {
	int a[10],n,i,pos,ele;
	printf("enter array size :");
	scanf("%d",&n);
	printf("enter array elements :");
       for(i=0;i<n;i++){
	scanf("%d",&a[i]);}
	printf("enter where ele added position :");
	scanf("%d",&pos);
	printf("enter  ele added  :");
	scanf("%d",&ele);
	for(i=n;i>pos;i--)
	a[pos-1]=ele;
	printf("after array element added array :");
	for(i=0;i<n;i++)
	printf("%d",a[i]);
	return 0;
}
