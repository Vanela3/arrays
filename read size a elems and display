#include<stdio.h>
int main()  {
    int a[9], n,i;
    printf("enter the size of array: ");
    scanf("%d",&n);
    printf ("enter the array the elements:");
    for ( i=0;i<n;i++){
    scanf("%d", &a[i]);}
    printf("array elements are :");
    for (i=0;i<n;i++) {
        printf("%d",a[i]);
         }
    return 0;
}
