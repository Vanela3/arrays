#include<stdio.h>
int main()  {
    int a[10], n,i, ele;
    printf("enter the size of array: ");
    scanf("%d",&n);
    printf ("enter the array the elements:");
    for ( i=0;i<n;i++){
    scanf("%d", &a[i]);}
    printf("array element to added is :");
    scanf("%d", &ele);
    a[0]=ele;
    n++;
    printf("after insert array the elements: ");
         for(i=0;i<n;i++){
             printf("%d", a[i]);
         }
    return 0;
}
